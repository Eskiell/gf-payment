{
    "name": "eskiell/gf-payment",
    "description": "SDK ",
    "keywords": [
        "eskiell",
        "gf-payment"
    ],
    "homepage": "https://github.com/eskiell/gf-payment",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Eskiell",
            "email": "eskiel@icloud.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Eskiell\\GfPayment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eskiell\\GfPayment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eskiell\\GfPayment\\GfPaymentServiceProvider"
            ],
            "aliases": {
                "GfPayment": "Eskiell\\GfPayment\\GfPaymentFacade"
            }
        }
    }
}
